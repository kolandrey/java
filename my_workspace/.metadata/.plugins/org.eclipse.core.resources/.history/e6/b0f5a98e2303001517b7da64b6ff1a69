package kostin.tema9;

public class Task9_2_2 {
	public static void fib(int n){
		for (int i = 0, fib1 = 0, fib2 = 1; i < n; i++) {
			int fib = fib1 + fib2;
			System.out.println(fib);
			fib2 = fib1;
			fib1 = fib;
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
