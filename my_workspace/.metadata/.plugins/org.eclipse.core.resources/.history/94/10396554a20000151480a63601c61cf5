/* Создайте массив из 11 случайных целых чисел из отрезка [-1;1], 
 * выведите массив на экран в строку. Определите какой элемент 
 * встречается в массиве чаще всего и выведите об этом сообщение 
 * на экран. Если два каких-то элемента встречаются одинаковое 
 * количество раз, то не выводите ничего.
 */

package kostin.tema8;

public class Task8_10 {

	public static void main(String[] args) {
		int mas[]=new int[11];
		for (int i = 0; i<mas.length; i++){
			mas[i] = (int)((Math.random()*3)-1);
			System.out.print(mas[i]+" ");
		}
	}

}
