package ua.kh.lessons.lesson5;

public class BMW extends Car {
	private boolean isHitChair;

	public BMW(String mark, int year, boolean isHitChair) {
		super(mark, year);
		this.isHitChair = isHitChair;
	}

	@Override
	public String toString() {
		return "BMW [ isHeatChair " + isHitChair + super.toString();
	}

}
