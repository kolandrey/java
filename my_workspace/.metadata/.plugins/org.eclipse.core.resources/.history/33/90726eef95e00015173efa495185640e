package temp;

class Main {
	public static int[] reverse3(int[] nums) {
		int[] temp = new int[nums.length];
		int count = 0;
		for (int i = nums[nums.length - 1]; i > 0; i--) {
			temp[count] = nums[i];
			count++;
		}
		return temp;
	}

	public static void main(final String[] args) {
		reverse3({1, 2, 3}) ;
		
	}
}