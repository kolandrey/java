package ua.kh.lessons.lesson6;

public class StaticTest extends Object {

	public static void main(String[] args) {
		Object p = new Person();
		Person.AVG_AGE = 100;
		Person.foo();
	}
}

class Person {
	public static int AVG_AGE = 65;
	public int age;
	private String name;

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Person() {
	}

	public Person(int age, String name) {
		this.age = age;
		this.name = name;
	}
	
	public static void foo(){
		System.out.println("123");
	}

}
