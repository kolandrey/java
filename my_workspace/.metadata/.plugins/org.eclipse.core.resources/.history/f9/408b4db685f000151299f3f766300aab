package ua.kh.lessons.lesson5;

public abstract class Car {
	private String mark;
	private int year;

	public String getMark() {
		return mark;
	}

	public void setMark(String mark) {
		this.mark = mark;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}

	public Car(String mark, int year) {
		this.mark = mark;
		this.year = year;
	}

	@Override
	public String toString() {
		return " mark=" + mark + ", year=" + year + "]";
	}

}
