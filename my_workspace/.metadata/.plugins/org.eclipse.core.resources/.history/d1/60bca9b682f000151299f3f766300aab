package ua.kh.lessons.lesson8;

import java.util.ArrayList;

public class ListTest {
	public static void main(String[] args) {
		ArrayList<Integer> list = new ArrayList<Integer>();
		list.add(1);
		list.add(11);
		list.add(21);
		list.add(10);
		list.add(10);
		list.add(1);
		list.add(31);
		list.add(4);
		System.out.println(list.toString());

		list.add(1, 100);
		System.out.println(list.toString());
		list.remove(0);
		System.out.println(list.toString());
		list.add(8, 200);
		System.out.println(list.toString());
	}
}
